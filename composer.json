{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
      "require": {
        "roots/wordpress-core-installer": "^1.0",
        "roots/wordpress": "5.2.1",
        "wpackagist-plugin/wordpress-seo": "11.3",
	      "wpackagist-plugin/redis-cache":"1.4.3",
        "wpackagist-plugin/amazon-s3-and-cloudfront":"2.1.1",
        "wpackagist-theme/twentynineteen": "1.4"
    },
      "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
          "content/plugins/{$name}/": ["type:wordpress-plugin"],
          "content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
          "content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
